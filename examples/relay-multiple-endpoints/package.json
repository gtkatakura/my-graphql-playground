{
  "name": "relay-multiple-endpoints",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 14.15.4"
  },
  "dependencies": {
    "@graphql-tools/schema": "^7.1.3",
    "@graphql-tools/stitch": "^7.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-plugin-relay": "^11.0.0",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.6.0",
    "react": "0.0.0-experimental-8af27aeed",
    "react-dom": "0.0.0-experimental-8af27aeed",
    "react-relay": "0.0.0-experimental-4c4107dd",
    "react-scripts": "4.0.3",
    "relay-runtime": "^11.0.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "relay": "relay-compiler",
    "relay:watch": "yarn relay --watch",
    "graphql:schema:update:sdl": "ts-node scripts/updateSchemaSdl.ts",
    "graphql:schema:download:all": "yarn graphql:schema:download:countries && yarn graphql:schema:download:weather",
    "graphql:schema:download:countries": "get-graphql-schema https://countries.trevorblades.com/ > ./src/__generated__/countries-schema.graphql",
    "graphql:schema:download:weather": "get-graphql-schema https://graphql-weather-api.herokuapp.com/ > ./src/__generated__/weather-schema.graphql"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-tools/merge": "^6.2.10",
    "@types/node": "^14.14.35",
    "@types/react-relay": "^11.0.0",
    "@types/relay-config": "^6.0.1",
    "chalk": "^4.1.0",
    "get-graphql-schema": "^2.1.2",
    "make-promises-safe": "^5.1.0",
    "relay-compiler": "^11.0.0",
    "relay-compiler-language-typescript": "^13.0.6",
    "relay-config": "^11.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
